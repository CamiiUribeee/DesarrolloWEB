---
interface Props {
	title: string;
}

const { title = "titulo bonito" } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head> 
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title> 
	</head>
	<body>
		<div class="bg-blue-500 h-10">
			preheader 
		</div>


		<header class="bg-orange-400 h-20 sticky top-0">
			

		</header>
		<menu class="flex bg-slate-400">
			<li>
				<a href="/" class="hover:bg-white/30 px-4 block">Inicio</a>
			</li>
			<li>
				<a href="/quienes-somos" class="hover:bg-white/30 px-4 block">Quienes somos</a>
			</li>
			<li>
				<a href="/organigrama" class="hover:bg-white/30 px-4 block">Organigrama</a>
			</li>
			<li>
				<a href="/contacto" class="hover:bg-white/30 px-4 block">Contacto</a>
			</li> 
		</menu>
		

		<slot /> 
	</body>
</html>
<style is:global>
@font-face {
  font-family: 'myfont';
  src: url('/fonts/NotoSansJP-VariableFont_wght.ttf') format('ttf');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
</style>
